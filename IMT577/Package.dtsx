              <DTS:ObjectData>
                <SQLTask:SqlTaskData
                  SQLTask:Connection="{1DACD363-E708-4327-A776-7FE95156F8FF}"
                  SQLTask:SqlStatementSource="IF EXISTS (SELECT TABLE_NAME FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_SCHEMA = 'dbo' AND TABLE_NAME = 'factSalesActual') &#xA;BEGIN&#xA;&#x9;-- ====================================&#xA;&#x9;-- This is split into three parts, one each for Stores, Resellers, and Customers.&#xA;&#x9;-- The code is the same for each part, save for which columns it checks for creating respective Keys.&#xA;&#x9;-- I know there's probably a better way to do this, but it works and I'm not getting graded for how efficient my code is.&#xA;&#x9;-- ====================================&#xA;&#xA;&#x9;-- 2) Three Conditional Loads (checking NULL values) to determine Store/Reseller/Customer Keys. Location Key used in all three.&#xA;&#xA;&#x9;-- Check for Stores&#xA;&#x9;INSERT INTO dbo.factSalesActual&#xA;&#x9;(&#xA;&#x9;dimStoreKey&#xA;&#x9;,dimResellerKey&#xA;&#x9;,dimCustomerKey&#xA;&#x9;,dimLocationKey&#xA;&#x9;,dimProductKey&#xA;&#x9;,dimChannelKey&#xA;&#x9;,dimSaleDateKey&#xA;&#x9;,SalesHeaderID&#xA;&#x9;,SalesDetailID&#xA;&#x9;,SaleAmount&#xA;&#x9;,SaleQuantity&#xA;&#x9;,SaleUnitPrice&#xA;&#x9;,SaleExtendedCost&#xA;&#x9;,SaleTotalProfit&#xA;&#x9;)&#xA;&#xA;&#x9;SELECT &#xA;&#x9;dimStore.dimStoreKey AS dimStoreKey&#xA;&#x9;,NULL AS dimResellerKey&#xA;&#x9;,NULL AS dimCustomerKey&#xA;&#x9;,dimStore.dimLocationKey AS dimLocationKey&#xA;&#x9;,dimProduct.dimProductKey AS dimProductKey&#xA;&#x9;,stageSalesHeader.ChannelID AS dimChannelKey&#xA;&#x9;,dimDate.dimDateID AS dimSaleDateKey&#xA;&#x9;,stageSalesHeader.SalesHeaderID AS SalesHeaderID&#xA;&#x9;,stageSalesDetail.SalesDetailID AS SalesDetailID&#xA;&#x9;,stageSalesDetail.SalesAmount AS SaleAmount&#xA;&#x9;,stageSalesDetail.SalesQuantity AS SaleQuantity&#xA;&#x9;,(stageSalesDetail.SalesAmount / stageSalesDetail.SalesQuantity) AS SaleUnitPrice&#xA;&#x9;,(stageSalesDetail.SalesQuantity * dimProduct.dimProductCost) AS SaleExtendedCost&#xA;&#x9;,(stageSalesDetail.SalesAmount - (stageSalesDetail.SalesQuantity * dimProduct.dimProductCost)) AS SaleTotalProfit&#xA;&#xA;&#x9;FROM stageSalesHeader&#xA;&#x9;LEFT JOIN stageSalesDetail&#xA;&#x9;ON stageSalesHeader.SalesHeaderID = stageSalesDetail.SalesHeaderID&#xA;&#x9;LEFT JOIN dimProduct&#xA;&#x9;ON stageSalesDetail.ProductID = dimProduct.dimProductID&#xA;&#x9;LEFT JOIN dimDate&#xA;&#x9;ON stageSalesHeader.Date = dimDate.FullDate&#xA;&#x9;LEFT JOIN dimStore &#xA;&#x9;ON stageSalesHeader.StoreID = dimStore.dimStoreID &#xA;&#x9;WHERE stageSalesHeader.StoreID IS NOT NULL&#xA;&#x9;;&#xA;&#xA;&#x9;-- Check for Resellers&#xA;&#x9;INSERT INTO dbo.factSalesActual&#xA;&#x9;(&#xA;&#x9;dimStoreKey&#xA;&#x9;,dimResellerKey&#xA;&#x9;,dimCustomerKey&#xA;&#x9;,dimLocationKey&#xA;&#x9;,dimProductKey&#xA;&#x9;,dimChannelKey&#xA;&#x9;,dimSaleDateKey&#xA;&#x9;,SalesHeaderID&#xA;&#x9;,SalesDetailID&#xA;&#x9;,SaleAmount&#xA;&#x9;,SaleQuantity&#xA;&#x9;,SaleUnitPrice&#xA;&#x9;,SaleExtendedCost&#xA;&#x9;,SaleTotalProfit&#xA;&#x9;)&#xA;&#xA;&#x9;SELECT &#xA;&#x9;NULL AS dimStoreKey&#xA;&#x9;,dimReseller.dimResellerKey AS dimResellerKey&#xA;&#x9;,NULL AS dimCustomerKey&#xA;&#x9;,dimReseller.dimLocationKey AS dimLocationKey&#xA;&#x9;,dimProduct.dimProductKey AS dimProductKey&#xA;&#x9;,stageSalesHeader.ChannelID AS dimChannelKey&#xA;&#x9;,dimDate.dimDateID AS dimSaleDateKey&#xA;&#x9;,stageSalesHeader.SalesHeaderID AS SalesHeaderID&#xA;&#x9;,stageSalesDetail.SalesDetailID AS SalesDetailID&#xA;&#x9;,stageSalesDetail.SalesAmount AS SaleAmount&#xA;&#x9;,stageSalesDetail.SalesQuantity AS SaleQuantity&#xA;&#x9;,(stageSalesDetail.SalesAmount / stageSalesDetail.SalesQuantity) AS SaleUnitPrice&#xA;&#x9;,(stageSalesDetail.SalesQuantity * dimProduct.dimProductCost) AS SaleExtendedCost&#xA;&#x9;,(stageSalesDetail.SalesAmount - (stageSalesDetail.SalesQuantity * dimProduct.dimProductCost)) AS SaleTotalProfit&#xA;&#xA;&#x9;FROM stageSalesHeader&#xA;&#x9;LEFT JOIN stageSalesDetail&#xA;&#x9;ON stageSalesHeader.SalesHeaderID = stageSalesDetail.SalesHeaderID&#xA;&#x9;LEFT JOIN dimProduct&#xA;&#x9;ON stageSalesDetail.ProductID = dimProduct.dimProductID&#xA;&#x9;LEFT JOIN dimDate&#xA;&#x9;ON stageSalesHeader.Date = dimDate.FullDate&#xA;&#x9;LEFT JOIN dimReseller &#xA;&#x9;ON stageSalesHeader.ResellerID = dimReseller.dimResellerID &#xA;&#x9;WHERE stageSalesHeader.ResellerID IS NOT NULL&#xA;&#x9;;&#xA;&#xA;&#x9;-- Check for Customers&#xA;&#x9;INSERT INTO dbo.factSalesActual&#xA;&#x9;(&#xA;&#x9;dimStoreKey&#xA;&#x9;,dimResellerKey&#xA;&#x9;,dimCustomerKey&#xA;&#x9;,dimLocationKey&#xA;&#x9;,dimProductKey&#xA;&#x9;,dimChannelKey&#xA;&#x9;,dimSaleDateKey&#xA;&#x9;,SalesHeaderID&#xA;&#x9;,SalesDetailID&#xA;&#x9;,SaleAmount&#xA;&#x9;,SaleQuantity&#xA;&#x9;,SaleUnitPrice&#xA;&#x9;,SaleExtendedCost&#xA;&#x9;,SaleTotalProfit&#xA;&#x9;)&#xA;&#xA;&#x9;SELECT &#xA;&#x9;NULL AS dimStoreKey&#xA;&#x9;,NULL AS dimResellerKey&#xA;&#x9;,dimCustomer.dimCustomerKey AS dimCustomerKey&#xA;&#x9;,dimCustomer.dimLocationKey AS dimLocationKey&#xA;&#x9;,dimProduct.dimProductKey AS dimProductKey&#xA;&#x9;,stageSalesHeader.ChannelID AS dimChannelKey&#xA;&#x9;,dimDate.dimDateID AS dimSaleDateKey&#xA;&#x9;,stageSalesHeader.SalesHeaderID AS SalesHeaderID&#xA;&#x9;,stageSalesDetail.SalesDetailID AS SalesDetailID&#xA;&#x9;,stageSalesDetail.SalesAmount AS SaleAmount&#xA;&#x9;,stageSalesDetail.SalesQuantity AS SaleQuantity&#xA;&#x9;,(stageSalesDetail.SalesAmount / stageSalesDetail.SalesQuantity) AS SaleUnitPrice&#xA;&#x9;,(stageSalesDetail.SalesQuantity * dimProduct.dimProductCost) AS SaleExtendedCost&#xA;&#x9;,(stageSalesDetail.SalesAmount - (stageSalesDetail.SalesQuantity * dimProduct.dimProductCost)) AS SaleTotalProfit&#xA;&#xA;&#x9;FROM stageSalesHeader&#xA;&#x9;LEFT JOIN stageSalesDetail&#xA;&#x9;ON stageSalesHeader.SalesHeaderID = stageSalesDetail.SalesHeaderID&#xA;&#x9;LEFT JOIN dimProduct&#xA;&#x9;ON stageSalesDetail.ProductID = dimProduct.dimProductID&#xA;&#x9;LEFT JOIN dimDate&#xA;&#x9;ON stageSalesHeader.Date = dimDate.FullDate&#xA;&#x9;LEFT JOIN dimCustomer&#xA;&#x9;ON stageSalesHeader.CustomerID = dimCustomer.dimCustomerID &#xA;&#x9;WHERE stageSalesHeader.CustomerID IS NOT NULL&#xA;&#x9;;&#xA;END&#xA;GO " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
