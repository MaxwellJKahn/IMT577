W IF EXISTS dbo.dimStoreView; &#xA;GO&#xA;&#xA;CREATE VIEW dimStoreView AS&#xA;SELECT dimStoreKey, dimLocationKey, dimStoreID, dimStoreName, dimStoreNumber, dimStoreManager&#xA;FROM dbo.dimStore;&#xA;GO&#xA;&#xA;-- factSalesActual&#xA;&#xA;DROP VIEW IF EXISTS dbo.factSalesActualView; &#xA;GO&#xA;&#xA;CREATE VIEW factSalesActualView AS&#xA;SELECT factSalesActualKey, dimProductKey, dimStoreKey, dimResellerKey, dimCustomerKey, dimChannelKey, dimSaleDateKey, dimLocationKey, SalesHeaderID, SalesDetailID, SaleAmount, SaleQuantity, SaleUnitPrice, SaleExtendedCost, SaleTotalProfit&#xA;FROM dbo.factSalesActual;&#xA;GO&#xA;&#xA;-- factSalesTarget&#xA;&#xA;DROP VIEW IF EXISTS dbo.factSalesTargetView; &#xA;GO&#xA;&#xA;CREATE VIEW factSalesTargetView AS&#xA;SELECT factSalesTargetKey, dimStoreKey, dimResellerKey, dimChannelKey, dimTargetDateKey, SalesTargetAmount&#xA;FROM dbo.factSalesTarget;&#xA;GO&#xA;&#xA;-- factProductSalesTarget&#xA;&#xA;DROP VIEW IF EXISTS dbo.factProductSalesTargetView; &#xA;GO&#xA;&#xA;CREATE VIEW factProductSalesTargetView AS&#xA;SELECT factProductSalesTargetKey, dimProductKey, dimTargetDateKey, ProductSalesTargetQuantity&#xA;FROM dbo.factProductSalesTarget;&#xA;GO&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
